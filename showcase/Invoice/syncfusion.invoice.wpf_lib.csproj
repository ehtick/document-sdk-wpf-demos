    <Content Include="Assets\Invoice\SmallLogo.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Assets\Invoice\SplashScreen.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Assets\Invoice\StoreLogo.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Assets\Invoice\Syncfusion.PNG">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
