    <Content Include="Assets\Invoice\Edit.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Assets\Invoice\Excel.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
