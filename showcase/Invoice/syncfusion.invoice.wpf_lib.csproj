      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Assets\Invoice\Word_Toast.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Compile Remove="Views\ExcelExport.cs" />
    <Compile Remove="Views\PdfExport.cs" />
    <Compile Remove="Views\WordExport.cs" />
    <None Remove="app.config" />
    <Compile Remove="App.xaml.cs" />
    <Compile Remove="Invoice.cs" />
    <Page Remove="InvoiceDemo.xaml" />
    <Compile Remove="InvoiceDemo.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
