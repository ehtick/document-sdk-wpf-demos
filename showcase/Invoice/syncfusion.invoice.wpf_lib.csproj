    <PackageReference Include="Syncfusion.DocIO.Wpf" Version="31.2.2." />
    <PackageReference Include="Syncfusion.ExcelChartToImageConverter.WPF" Version="31.2.2." />
    <PackageReference Include="Syncfusion.ExcelToPdfConverter.Wpf" Version="31.2.2." />
