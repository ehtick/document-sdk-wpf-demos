    </Content>
    <Content Include="Assets\Presentation\MergeContent.pptx">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Assets\Presentation\Syncfusion Presentation.pptx">
