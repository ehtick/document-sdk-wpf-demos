      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Assets\Presentation\FindAndHighlightInput.pptx">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <None Remove="App.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="31.2.2." />
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="31.2.2." />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="31.2.2." />
    <PackageReference Include="Syncfusion.Presentation.Wpf" Version="31.2.2." />
    <PackageReference Include="Syncfusion.PresentationToPdfConverter.Wpf" Version="31.2.2." />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="31.2.2." />
